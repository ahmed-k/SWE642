<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
        <package name="survey" extends="struts-default">

                <action name="survey" class="survey.action.SurveyAction" method="execute">
                        <result name="simple">SimpleAcknowledgment.jsp</result>
                        <result name="winner">WinnerAcknowledgment.jsp</result>
                        <result name="error">ErrorPage.jsp</result>
                </action>
                <action name="student" class="survey.StudentAction" method="execute">
                       <result name="success">ShowParticularStudent.jsp</result>
                       <result name="error">ErrorPage.jsp</result>
                </action>
        </package>
</struts>